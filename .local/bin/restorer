
#!/bin/bash
SESSION_FILE="$HOME/.local/share/brave_session.json"

# Check if the session file exists
if [[ ! -f "$SESSION_FILE" ]]; then
  echo "No session file found. Please save a session first."
  exit 1
fi

# Read the URLs from the session file
URLS=$(cat "$SESSION_FILE")

# Launch Brave with the saved URLs
brave $URLS
